section.layer.text.themeonetext {

    display: grid;

    grid-template-rows: auto auto 1fr auto auto;

    border: solid transparent

}



section.layer.text.themeonetext div {

    color: var(--style-text);

    letter-spacing: .05;

    text-align: center

}



section.layer.text.themeonetext div:empty {

    margin-bottom: unset

}



section.layer.text.themeonetext .headline {

    margin-bottom: .375em;

    grid-row: 1;

    font-size: 1em;

    font-weight: 600

}



section.layer.text.themeonetext .subtitle {

    grid-row: 2;

    font-size: .5em;

    font-weight: 400

}



section.layer.text.themeonetext .footnote {

    margin-bottom: .5em;

    grid-row: 4;

    font-size: .5em;

    font-weight: 600

}



section.layer.text.themeonetext .metadata {

    grid-row: 5;

    font-size: .4em;

    font-weight: 400

}



html.font_family-circular section.layer.text.themeonetext {

    font-family: "circular"

}



