section.layer.elevation {

    display: flex;

    align-items: flex-end;

    flex-direction: row

}



section.layer.elevation canvas {

    min-width: 0;

    flex: 1;

    image-rendering: crisp-edges

}



section.layer.elevation canvas:not(:last-child) {

    margin-right: 1px

}



