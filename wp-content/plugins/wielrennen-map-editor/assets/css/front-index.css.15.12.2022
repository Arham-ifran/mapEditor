.action {

    display: flex;

    flex-direction: row;

}



.action a.button {

    flex: 1;

}



@media(max-width: 800px) {

    .action a.button {

        border-radius: unset !important;

    }

}



@media(min-width: 801px) {

    .action a.button:not(:last-child) {

        margin-right: 24px;

    }

}



a.button {

    display: inline-flex;

    width: 100%;

    padding: 0 10px;

    align-items: center;

    justify-content: center;

    flex-direction: row;

    text-decoration: none;

}



a.button.large {

    height: 48px;

    text-transform: uppercase;

    border-radius: 3px;

    font-weight: 600;

    letter-spacing: .05em;

    font-size: 15px;

}





a.button.small {

    height: 38px;

    border-radius: 50vw;

    font-weight: 400;

    letter-spacing: .0375em;

    font-size: 14px;

}



@media(max-width: 800px) {

    a.button.small {

        font-size: 12px;

    }

    a.button.small {

        height: 34px;

    }

}



a.button.primary {

    color: #fff;

    background-color: #eb7025;

    transition: background-color 200ms;

    will-change: background-color;

}



@media(pointer: fine) {

    a.button.primary:hover {

        background-color: #eb7025;

    }

}



@media(min-width: 801px) {

    a.button.secondary {

        color: #00b15d;

        border: 1px solid;

        transition: color 200ms;

        will-change: color;

    }

}



@media(max-width: 800px) {

    a.button.secondary {

        color: #fff;

        background-color: #00b15d;

        transition: background-color 200ms;

        will-change: background-color;

    }

}



@media(pointer: fine)and (min-width: 801px) {

    a.button.secondary:hover {

        color: #eb7025;

    }

}



@media(pointer: fine)and (max-width: 800px) {

    a.button.secondary:hover {

        background-color: #eb7025;

    }

}



a.button.tertiary {

    color: #fff;

    background-color: #eb7025;

    transition: 0.3s;

}



a.button.tertiary.custom-outline-button{

    color: #000;

    background-color: transparent;

    border: 2px solid #000;

}

a.button.tertiary.custom-outline-button:hover{

    background-color: #eb7025;

    border-color: #eb7025;

    color: #fff;

}

a.button.disabled {

    filter: grayscale(100%);

    opacity: .5;

    pointer-events: none;

}



span.emoji {

    font-family: Apple Color Emoji, Segoe UI Emoji, Segoe UI Symbol;

    font-size: 1em;

    font-weight: 400;

    line-height: 1;

    vertical-align: -0.075em;

}



.field.search {

    position: relative;

}



.field.search::after {

    position: absolute;

    top: 50%;

    right: 0;

    display: block;

    width: 18px;

    height: 18px;

    margin: -9px 16px 0;

    content: "";

    background-color: #000;

    -webkit-mask-image: url("../images/magnify.svg");

    mask-image: url("../images/magnify.svg");

    -webkit-mask-position: center;

    mask-position: center;

    -webkit-mask-size: contain;

    mask-size: contain;

    -webkit-mask-repeat: no-repeat;

    mask-repeat: no-repeat;

}



.field.search input {

    width: 100%;

    height: 48px;

    padding: 0 50px 0 16px;

    color: #000;

    border: 1px solid #979b9b;

    border-radius: 3px;

    font-weight: 400;

    letter-spacing: .0375em;

    font-size: 14px;

}



@media(max-width: 800px) {

    .field.search input {

        font-size: 12px;

    }

}



.field.search input::-moz-placeholder {

    color: #979b9b;

}



.field.search input:-ms-input-placeholder {

    color: #979b9b;

}



.field.search input::placeholder {

    color: #979b9b;

}



html.search_active .field.search::after {

    -webkit-mask-image: url("../images/loading.svg");

    mask-image: url("../images/loading.svg");

    -webkit-animation-name: load;

    animation-name: load;

    -webkit-animation-duration: 1000ms;

    animation-duration: 1000ms;

    -webkit-animation-iteration-count: infinite;

    animation-iteration-count: infinite;

    -webkit-animation-timing-function: linear;

    animation-timing-function: linear;

}



@-webkit-keyframes load {

    from {

        transform: rotate(0deg)

    }

    to {

        transform: rotate(360deg);

    }

}



@keyframes load {

    from {

        transform: rotate(0deg);

    }

    to {

        transform: rotate(360deg);

    }

}



.field.text:not(:last-child) {

    margin-bottom: 8px;

}



.field.text input {

    position: relative;

    width: 100%;

    height: 48px;

    padding: 0 16px;

    color: #000;

    border: 2px solid #e1e1e1;

    font-weight: 400;

    letter-spacing: .0375em;

    font-size: 14px;

}



@media(max-width: 800px) {

    .field.text input {

        font-size: 12px;

    }

}



.field.text input:focus {

    border-color: #484c4c;

}



.field.text input::-moz-placeholder {

    color: #979b9b;

}



.field.text input:-ms-input-placeholder {

    color: #979b9b;

}



.field.text input::placeholder {

    color: #979b9b;

}



.field.toggle {

    display: flex;

    height: 20px;

    align-items: center;

    flex-direction: row;

}



.field.toggle input {

    position: fixed !important;

    width: 1px;

    height: 1px;

    padding: 0;

    margin: -1px;

    overflow: hidden;

    clip: rect(0 0 0 0);

}



.field.toggle label {

    position: relative;

    padding-left: 32px;

    color: #000;

    cursor: pointer;

    font-weight: 500;

    letter-spacing: .0375em;

    font-size: 12px

}



@media(max-width: 800px) {

    .field.toggle label {

        font-size: 10px

    }

}



.field.toggle label::before,

.field.toggle label::after {

    position: absolute;

    top: 50%;

    left: 0;

    display: block;

    transform: translateY(-50%)

}



.field.toggle label::before {

    width: 20px;

    height: 20px;

    content: "";

    border: 1px solid #484c4c;

    border-radius: 3px

}



.field.toggle input:checked+label::before {

    background-color: #eb7025;

    border-color: #eb7025;

}



.field.toggle label::after {

    width: 12px;

    height: 12px;

    margin: 0 4px;

    background-color: #fff;

    -webkit-mask-image: url("../images/toggle.svg");

    mask-image: url("../images/toggle.svg");

    -webkit-mask-position: center;

    mask-position: center;

    -webkit-mask-repeat: no-repeat;

    mask-repeat: no-repeat

}



.field.toggle input:checked+label::after {

    content: ""

}



.field .note {

    position: relative;

    padding: 6px 6px 6px 2.75em;

    color: #000;

    background-color: #e1e1e1;

    font-weight: 400;

    font-size: 12px;

    margin-top:  20px;

}



@media(max-width: 800px) {

    .field .note {

        font-size: 9px

    }

}



.field .note::before,

.field .note::after {

    position: absolute;

    top: 50%;

    left: 0;

    display: block;

    content: "";

    transform: translateY(-50%)

}



.field .note::before {

    width: 1em;

    height: 1em;

    margin: 0 8px;

    background-color: #fff;

    border-radius: 50%

}



.field .note::after {

    width: 1.4em;

    height: 1.4em;

    margin: 0 6px;

    background-color: #000;

    -webkit-mask-image: url("../images/note.svg");

    mask-image: url("../images/note.svg");

    -webkit-mask-position: center;

    mask-position: center;

    -webkit-mask-size: contain;

    mask-size: contain;

    -webkit-mask-repeat: no-repeat;

    mask-repeat: no-repeat

}



.field .note strong {

    font-weight: 700

}



.group {

    width: 100%;

    transition:  1s ease all;

}



.group:not(:first-child) {

    padding-top: 20px;

    margin-top: 20px

}







.group a.item {

    border: 2px solid #e1e1e1;

    border-radius: 3px;

    transition: border-color 100ms;

    will-change: border-color

}



@media(pointer: fine) {

    .group a.item:hover {

        border-color: #979b9b

    }

}





.strava section.list a.item.active a.toggle{

background-color: #fff;

}

.group>h3 {

    margin-bottom: .75em;

    color: #000;

    text-align: center;

    text-transform: uppercase

}

.show-group-lists .group>h3{

    text-align: left;

    margin-bottom: 20px;

}

.show-group-lists .group .group .item{

    border: 2px solid #ddd;

}

.group>p {

    display: flex;

    margin-top: .75em;

    align-items: center;

    flex-direction: column;

