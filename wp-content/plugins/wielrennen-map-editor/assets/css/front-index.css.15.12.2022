/* css modal */


.modal-window {
    position: fixed;
    background-color: rgba(255, 255, 255, 0.25);
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 999;
    visibility: hidden;
    opacity: 0;
    pointer-events: none;
    transition: all 0.3s;
  }
  .modal-window.modal-active {
    visibility: visible;
    opacity: 1;
    pointer-events: auto;
    background: #0000009e;
  }
  .modal-window > div {
    width: 400px;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    background: white;
  }
  .custom-map-modal{
    padding: 0 2em 2em;
  }
  .modal-window header {
    font-weight: bold;
  }
  .modal-window h1 {
    font-size: 150%;
    margin: 0 0 15px;
    background: #e85308;
    padding: 20px;
    color: #fff;
    border-radius: 12px 8px 0 0;
}
label.form-label {
    line-height: 20px;
    padding-bottom: 34px;
}
  .modal-close {
    color: #aaa;
    line-height: 50px;
    font-size: 80%;
    position: absolute;
    right: 0;
    text-align: center;
    top: 0;
    width: 70px;
    text-decoration: none;
  }
  .modal-close:hover {
    color: black;
  }
  
  /* Demo Styles */
  .ccontainer {
    display: grid;
    justify-content: center;
    align-items: center;
    height: 100vh;
  }
  
  .modal-window > div {
    border-radius: 1rem;
  }
  .modal-window div:not(:last-of-type) {
    margin-bottom: 15px;
  }
  .close-modal-top-btn {
    color: #fff;
    position: absolute;
    background-color: #e85308;
    opacity: 1;
    background-image: none;
    padding: 15px;
    right: -15px;
    top: -18px;
    border-radius: 50%;
    z-index: 1;
    font-size: 20px;
    width: 20px;
    height: 20px;
    display: flex;
    justify-content: center;
    align-items: center;
}
.close-modal-top-btn span{
    position: absolute;
    font-size: 40px;
}

.gpx_product_update {
    padding: 10px;
    background: #e85308;
    color: #fff;
    border-radius: 5px;
}

